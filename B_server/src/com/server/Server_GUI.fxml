            <Button fx:id="browse" layoutX="387.0" layoutY="22.0" mnemonicParsing="false" onAction="#browseButtonAction" text="Browse" />
            <Button fx:id="start_server" layoutX="458.0" layoutY="22.0" mnemonicParsing="false" onAction="#start_serverButtonAction" text="Start Server" />
            <TextArea fx:id="textArea" layoutX="16.0" layoutY="64.0" prefHeight="207.0" prefWidth="522.0" />
