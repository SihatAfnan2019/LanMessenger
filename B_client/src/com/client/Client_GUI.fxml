            <Label fx:id="host_address" layoutX="6.0" layoutY="134.0" text="Host Address :" />
            <Label fx:id="host_port" layoutX="313.0" layoutY="133.0" text="Host Port :" />
            <TextField fx:id="host_addresstxt" layoutX="98.0" layoutY="130.0" prefHeight="25.0" prefWidth="212.0" />
