<GridPane alignment="center" hgap="10" prefHeight="643.0" prefWidth="670.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.client.Client_Controller">
   <columnConstraints>
      <ColumnConstraints />
